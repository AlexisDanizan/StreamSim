<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
<servlet>
	<servlet-name>Generator</servlet-name>
	<servlet-class>servlets.Generator</servlet-class>
</servlet>

<servlet>
	<servlet-name>Loader</servlet-name>
	<servlet-class>servlets.Loader</servlet-class>
</servlet>

<servlet>
	<servlet-name>Emitter</servlet-name>
	<servlet-class>servlets.Emitter</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Generator</servlet-name>
	<url-pattern>/generator</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Loader</servlet-name>
	<url-pattern>/loader</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Emitter</servlet-name>
	<url-pattern>/emitter</url-pattern>
</servlet-mapping>
</web-app>